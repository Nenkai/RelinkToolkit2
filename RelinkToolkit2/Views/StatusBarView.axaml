<UserControl x:Class="RelinkToolkit2.Views.StatusBarView"
			 xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:viewmodels="using:RelinkToolkit2.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="20"
             x:DataType="viewmodels:StatusBarViewModel">
	<Design.DataContext>
		<viewmodels:SolutionExplorerViewModel/>
	</Design.DataContext>
	<Label Content="{Binding Message}"/>
</UserControl>
